
<%- include('header') %>    
<a href="/credentials/add">Add Record</a>
<% if(data.length) { %>

    <table class="table table-bordered">
        <thead>
            <th>Website</th>
            <th>Service</th>
            <th>Username</th>
            <th>Action</th>
        </thead>
        <tbody>
            <% for(var i=0; i < data.length; i++) { %>
                <tr>
                    <td><%= data[i].website %></td>
                    <td><%= data[i].service %></td>
                    <td><%= data[i].username %></td>
                    <td>
                        <a class="btn btn-primary btn-small" href="/credentials/show/<%= data[i].id %>">Show</a>
                        <a class="btn btn-primary btn-small" href="/credentials/update/<%= data[i].id %>">Edit</a>
                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>
<% } %>

<%- include('footer') %>    